{
  "name": "Actron Air Conditioner",
  "version": "0.4",
  "slug": "hass-actron",
  "description": "An add-on for Actron Air Conditioners using the Actron Connect Wi-Fi module.",
  "url": "https://github.com/MikeJMcGuire/hass-actron",
  "startup": "before",
  "boot": "auto",
  "image": "mikejmcguire/hass-actron-{arch}",
  "arch": [
    "amd64",
    "armhf",
    "i386"
  ],
  "options": {
    "MQTTUser": "mqttuser",
    "MQTTPassword": "mqttpassword",
    "MQTTBroker": "core-mosquitto",
    "Zones": [
      {
        "Name": "Zone 1",
        "Id": 1
      },
      {
        "Name": "Zone 2",
        "Id": 2
      }
    ]
  },
  "schema": {
    "MQTTUser": "str?",
    "MQTTPassword": "str?",
    "MQTTBroker": "str",
    "ForwardToInternalWebService": "str?",
    "Zones": [
      {
        "Name": "str",
        "Id": "int"
      }
    ]
  },
  "ports": {
    "80/tcp": 80
  }
}
